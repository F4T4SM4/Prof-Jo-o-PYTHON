# Início
custo_fabrica = float(input("Digite o custo de fábrica do carro: "))

# Fixos
percentual_distribuidor = 28  # em porcentagem
percentual_impostos = 45      # em porcentagem

# Cálculo
valor_distribuidor = (percentual_distribuidor / 100) * custo_fabrica
valor_impostos = (percentual_impostos / 100) * custo_fabrica
custo_final = custo_fabrica + valor_distribuidor + valor_impostos

# Resultado
print("O custo final ao consumidor é: R$ {:.2f}".format(custo_final))
