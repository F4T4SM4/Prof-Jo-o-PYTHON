#inicio 
total_eleitores = int(input("qual Ã© o total de eleitres: "))
votos_brancos = int(input("Quantos votos em branco: "))
votos_nulos = int(input("Quantos vatos nulos: "))
votos_validos = int(input("quantos votos validados: "))

#conta
percentual_brancos = (votos_brancos / total_eleitores) * 100
percentual_nulos = (votos_nulos / total_eleitores) * 100
percentual_validos = (votos_validos / total_eleitores) * 100

#resultado
print("toal de votos: {:.2f}%".format(percentual_validos))
print("total de votos em branco: {:.2f}%".format(percentual_brancos))
print("total de votos nulos: {:.2f}%".fotmat(percentual_nulos))
